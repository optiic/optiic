{
  "name": "optiic",
  "version": "1.0.1",
  "description": "An advanced free OCR & image recognition API.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/optiic/optiic"
  },
  "keywords": [
    "ocr",
    "ocr api",
    "image api",
    "image recognition",
    "image recognition api",
    "captcha",
    "captcha solver",
    "captcha api",
    "rest api"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/optiic/optiic/issues"
  },
  "homepage": "https://optiic.dev",
  "runkitExampleFilename": "examples/process.js",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "form-data": "^3.0.1",
    "itwcw-package-analytics": "^1.0.4",
    "wonderful-fetch": "^1.1.12"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.12"
  }
}